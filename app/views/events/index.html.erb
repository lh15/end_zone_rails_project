<%= image_tag  "concert_crowd.jpg", :class => "banner_img" %>

  <div id="searchdiv">
    <div id="searchbar">
      <input type="text" placeholder="Search for events, artists, teams or venues">
    </div>
  </div>

  <div id="loadingDiv" class="preloader-wrapper big active">
    <div class="spinner-layer spinner-blue-only">
      <div class="circle-clipper left">
        <div class="circle"></div>
      </div>
      <div class="gap-patch">
        <div class="circle"></div>
      </div>
      <div class="circle-clipper right">
        <div class="circle"></div>
      </div>
    </div>
  </div>

  </div>
  <div id="display_events">

  </div>
  <div class="row">
    <% @events.each do |event| %>
      <a href="/events/<%= event.id %>">

        <div class="col s12 m3 ">
          <div class="card">
            <div class="card-image">
              <%= image_tag  "ticket_1_.png", :class => "image_in_card" %>
            </div>
            <div class="card-content">
              <p>
                <%= event.title %>
              </p>
            </div>
          </div>
        </div>


      </a>
      <% end %>
  </div>